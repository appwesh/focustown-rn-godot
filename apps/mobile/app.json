{
  "expo": {
    "name": "Focus Town",
    "slug": "focustown",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "focustown",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "apps.wesh.focustown",
      "appleTeamId": "VY92D2R4AT",
      "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": [
        "applinks:focustown.app"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "app-1-437937185673-ios-2f5373fc5a91ab6f7e7758"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F5EFE6",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.edgarhnd.focustown",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "focustown.app",
              "pathPrefix": "/YOUR_TEMPLATE_ID"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F5EFE6",
          "dark": {
            "backgroundColor": "#2C3639"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "mode": "development"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "./plugins/with-firebase-godot-fix.js",
      "expo-audio",
      [
        "react-native-appsflyer",
        {
          "shouldUseStrictMode": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "940ef7e3-e856-46bd-8fcf-39c65583a5ed"
      }
    }
  }
}
